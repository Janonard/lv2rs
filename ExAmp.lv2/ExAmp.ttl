@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

<https://github.com/Janonard/ExAmp>
        a lv2:Plugin , lv2:AmplifierPlugin ;
        lv2:project <http://github.com/Janonard> ;
        doap:name "ExAmp";
        doap:license <http://opensource.org/licenses/isc> ;
        lv2:port [
                a lv2:InputPort ,
                        lv2:ControlPort ;
                lv2:index 0 ;
                lv2:symbol "delay" ;
                lv2:name "Delay" ;
                units:unit units:db ;
                lv2:default 1.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 200.0 ;
        ] , [
                a lv2:AudioPort ,
                        lv2:InputPort ;
                lv2:index 1 ;
                lv2:symbol "in" ;
                lv2:name "In"
        ] , [
                a lv2:AudioPort ,
                        lv2:OutputPort ;
                lv2:index 2 ;
                lv2:symbol "out" ;
                lv2:name "Out"
        ] .
